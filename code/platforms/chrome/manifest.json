{
  "name": "Levenshtein Challenge",
  "description": "A simple game in which you must calculate the edit distance between two words",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "64": "img/icon_128.png",
    "128": "img/icon_128.png",
    "256": "img/icon_128.png",
    "512": "img/icon_128.png"
  },
  "offline_enabled": true,
  "permissions": [
      "storage"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  }
}
<?xml version='1.0' encoding='utf-8'?>
<widget id="levenshtein.jfmdev.github.io" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LevenshteinChallenge</name>
    <description>
        A simple game in which you must calculate the 'edit distance' between two words
    </description>
    <author email="kudalab@inboxalias.com" href="http://jfmdev.github.io">
        JFMDev
    </author>
    <icon src="res/icon_128.png" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/icon_64.png" density="ldpi" />
        <icon src="res/icon_128.png" density="mdpi" />
        <icon src="res/icon_256.png" density="hdpi" />
        <icon src="res/icon_512.png" density="xhdpi" />
    </platform>
    <platform name="firefoxos">
        <icon src="res/icon_60.png" width="60" height="60" />
        <icon src="res/icon_128.png" width="128" height="128" />
     </platform>
</widget>
